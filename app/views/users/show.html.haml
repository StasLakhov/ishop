%p{id:"notice"}= notice

.container
  .panel.panel-default
    .panel-heading
      = t('.users')
    %table.table
      %thead
        %tr
          %th= t('.name')
          %th= t('.age')

      %tbody
        %tr
          %td= @user.name
          %td= @user.age
          %td= link_to 'Edit', edit_user_path(@user), class: 'btn btn-success'
  %br
  %td= link_to t('.back'), users_path, class: 'btn btn-default'